Arbre sous_arbre_NO(Arbre arbre){
    Arbre res=malloc(sizeof(Arbre));
    if(is_noeud(arbre)){
        res=arbre.NO;
        res=res->fils[];  /* Pas sur s'il faut utilisé la structure comme ca ou pas */
    }
    return res;
}
Arbre sous_arbre_NE(Arbre arbre){
